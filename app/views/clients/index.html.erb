<%- @page_title = "Clients" %>
<h1>Clients</h1>
<div id="search_box" style="text-align:right">
  <%= render :partial => 'shared/search_box', :locals => { :objects => @clients } %>
</div>
<div id="results_table">
<%- if @clients.size > 0 -%>
  <%= render :partial => 'shared/results_table', :locals => { :objects => @clients } %>
<%- else -%>
  No data
<%- end -%>
</div>
