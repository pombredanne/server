<tr class="<%= cycle('odd', 'even') -%>">
  <td nowrap="nowrap" width="5%">
    <%= link_to 'View', {:action=>"show", :id => item.id}  %> 
  </td>
  <td><%= h(item.name) %></td>
  <td><%= h(item.version) %></td>
  <td><%= h(item.package_version) %></td>
  <td> <%= link_to h(item.filename), {:action=>"download", :filename => item.filename}  %></td>
  <td><%= h(item.maintainer) %></td>
  <td><%= h(item.os) %></td>
  <td><%= h(item.arch) %></td>
  <td><%= link_to "#{h(item.client_packages.size)} client(s)",{:action=>"show", :id => item.id} %></td>
</tr>
